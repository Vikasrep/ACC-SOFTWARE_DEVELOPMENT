{
	"name": "pcu-software",
	"version": "0.0.0",
	"description": "software for pcu employees",
	"main": "index.js",
	"keywords": [
		"pointcomfort"
	],
	"author": "point comfort underwriters",
	"license": "ISC",
	"engines": {
		"node": ">=14",
		"npm": ">=7"
	},
	"scripts": {
		"prepare": "husky install",
		"test": "yarn test:chromium",
		"test:debug": "run this via command line: PWDEBUG=console yarn test",
		"test:debug:file": "run this via command line: PWDEBUG=console npx playwright test filename",
		"test:all": "playwright test --browser=all",
		"test:chromium": "playwright test --browser=chromium",
		"test:webkit": "playwright test --browser=webkit",
		"test:firefox": "playwright test --browser=firefox",
		"start": "webpack-dev-server --node-env none --hot --open",
		"restart": "webpack-dev-server --node-env none --hot",
		"build:local": "webpack --node-env none",
		"build:staging": "webpack --node-env development",
		"build:production": "webpack --node-env production --no-devtool --performance-hints false",
		"jsonlint": "jsonlint .prettierrc.json --sort-keys --in-place && jsonlint .eslintrc.json --sort-keys --in-place && jsonlint .stylelintrc.json --sort-keys --in-place",
		"eslint": "eslint **/*.js",
		"eslint:fix": "eslint --fix **/*.js",
		"prettier": "prettier --check --write .",
		"stylelint": "stylelint **/*.scss",
		"stylelint:fix": "stylelint **/*.scss --fix",
		"husky": "yarn jsonlint && yarn stylelint && yarn prettier && yarn eslint",
		"husky:test": "yarn test:chromium --workers 1",
		"husky:fix": "yarn jsonlint && yarn stylelint:fix && yarn prettier && yarn eslint:fix",
		"install:clean": "rm -f yarn.lock && rm -rf node_modules && yarn install",
		"taskkill": "cmd \"/C TASKKILL /F /IM node.exe\"",
		"create:env": "cp ./.env-example.json ./.env.json"
	},
	"dependencies": {
		"@material-ui/core": "^4.12.4",
		"@material-ui/icons": "^4.11.3",
		"@material-ui/lab": "^4.0.0-alpha.61",
		"@reduxjs/toolkit": "^1.8.1",
		"bootstrap": "^5.1.3",
		"i18next": "^21.6.16",
		"prop-types": "^15.8.1",
		"react": "^18.0.0",
		"react-bootstrap": "^2.2.3",
		"react-data-grid": "^7.0.0-beta.12",
		"react-dom": "^18.0.0",
		"react-i18next": "^11.16.6",
		"react-papaparse": "^4.1.0",
		"react-redux": "^8.0.0",
		"react-router-dom": "^6.3.0",
		"redux": "^4.1.1",
		"redux-logger": "^3.0.6",
		"validator": "^13.7.0",
		"whatwg-fetch": "^3.6.2"
	},
	"devDependencies": {
		"@babel/core": "^7.17.9",
		"@babel/plugin-transform-runtime": "^7.17.0",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-react": "^7.16.7",
		"@playwright/test": "^1.21.0",
		"babel-loader": "^8.2.4",
		"compression-webpack-plugin": "^9.2.0",
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^6.7.1",
		"css-minimizer-webpack-plugin": "^3.4.1",
		"dotenv-json": "^1.0.0",
		"eslint": "^8.13.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-playwright": "^0.8.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.4.0",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.5.0",
		"husky": "^7.0.4",
		"jsonlint": "^1.6.3",
		"mini-css-extract-plugin": "^2.6.0",
		"prettier": "^2.6.2",
		"react-hot-loader": "^4.13.0",
		"sass": "^1.50.0",
		"sass-loader": "^12.6.0",
		"stylelint": "^14.7.1",
		"stylelint-config-prettier-scss": "^0.0.1",
		"stylelint-config-standard-scss": "^3.0.0",
		"webpack": "^5.72.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.8.1"
	}
}
